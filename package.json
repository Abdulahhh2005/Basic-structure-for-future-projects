{
  "name": "parcel-starter",
  "version": "1.0.0",
  "scripts": {
    "start": "parcel ./src/index.html --open",
    "build": "parcel build --public-url=/dist/",
    "clean": "npx rimraf dist .parcel-cache",
    "update": "npm install parcel@latest prettier@latest rimraf@latest --save-dev",
    "format": "prettier --write \"src/**/*.{js,json,css,scss,html}\""
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.12.0",
    "parcel": "^2.15.4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1"
  },
  "browserslist": [
    "defaults"
  ]
}
